<%- include('../partials/header') %>

<table id="list">
    <thead>
      <tr>
        <th>Palace</th>
        <th>LOCATION</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% palaces.forEach(function(m) { %>
        <tr>
          <td><%= m.palaceName %></td>
          <td><%= m.location %></td>
          <td><a href="/palaces/<%= m._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>

<%- include('../partials/footer') %>