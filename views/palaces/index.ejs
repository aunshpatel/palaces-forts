<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Palace/Fort Name</th>
      <th>Location (City)</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% palaces.forEach(function(p) { %>
      <tr>
        <td><%= p.palaceName %></td>
        <td><%= p.location %></td>
        <td><a href="/palaces/<%= p._id %>">DETAILS</a></td>
      </tr>
    <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>